{
    "name": "n8n-nodes-async-http-request",
    "version": "0.1.0",
    "description": "An n8n node to send HTTP requests and wait for asynchronous webhook responses.",
    "main": "dist/nodes/AsyncHttpRequest/AsyncHttpRequest.node.js",
    "scripts": {
      "build": "tsc && gulp build:icons",
      "dev": "tsc --watch",
      "lint": "eslint nodes/**/*.ts",
      "format": "prettier --write nodes/**/*.ts",
      "prepare": "npm run build"
    },
    "files": [
      "dist"
    ],
    "keywords": [
      "n8n-community-node-package",
      "n8n",
      "http",
      "request",
      "async",
      "webhook"
    ],
    "author": "Your Name or Organization",
    "license": "MIT",
    "homepage": "https://github.com/your-repo/n8n-nodes-async-http-request#readme",
    "repository": {
      "type": "git",
      "url": "git+https://github.com/your-repo/n8n-nodes-async-http-request.git"
    },
    "bugs": {
      "url": "https://github.com/your-repo/n8n-nodes-async-http-request/issues"
    },
    "n8n": {
      "n8nNodesApiVersion": 1,
      "nodes": [
        "dist/nodes/AsyncHttpRequest/AsyncHttpRequest.node.js"
      ]
    },
    "dependencies": {
        "n8n-core": "latest",
        "n8n-workflow": "latest"
    }
}